<div id="<%= dom_id shop %>">
  <p>
    <strong>Category:</strong>
    <%= shop.category.name %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= shop.name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= shop.address %>
  </p>



  <%= content_tag(:iframe, 'map', src:'https://www.google.com/maps/embed/v1/place?key='+ ENV['GOOGLE_MAP_API_KEY']  + '&q=' + shop.address, width: 800, height: 400, frameborder: 0) %>

</div>